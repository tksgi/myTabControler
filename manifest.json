{
    // Required
    "manifest_version":2,
    "name": "Tab Controll",
    "version": "0.1",
    "description": "開きすぎたタブを整理する拡張機能",
    
    "browser_action": {
        "default_icon": {
            "19": "icon.png"
        },
        "default_popup": "popup.html"
    },
    
    "permissions": ["tabs"]
    
    
}